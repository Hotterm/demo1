#include<stdio.h>

int calulate(int Long, int Width ,int Height);

int main()
{
	int m_Long;
	int m_width;
	int Height;
	int result;

	printf("请输入长度\n");
	scanf_s("%d", &m_Long);

	printf("请输入宽度\n");
	scanf_s("%d", &m_width);

	printf("请输入高度\n");
	scanf_s("%d", &Height);

	result = m_Long * m_width * Height;
	printf("长方形的体积是:");
		printf("%d",result);
	return 0;
}

int caluate(int Long, int Width ,int Height)
{
	int result = Long * Width * Height;
	return result;
}
